[Script]
# 播放器会员皮肤
http-request ^https:\/\/interface.+music\.163\.com\/eapi\/playermode\/ script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.jss, tag = 播放器会员皮肤
# 搜索结果会员歌曲
http-request ^https:\/\/interface.+music\.163\.com\/eapi\/search\/complex\/(page|rec\/song\/get) script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 搜索结果会员歌曲
# 播放器会员歌曲
http-request ^https:\/\/interface.+music\.163\.com\/eapi\/v3\/song\/detail script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 播放器会员歌曲
http-request ^https:\/\/interface.+music\.163\.com\/eapi\/song\/(chorus|enhance\/|play\/|type\/detail\/get) script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 播放器会员歌曲
http-request ^https:\/\/interface.+music\.163\.com\/eapi\/(v1\/artist\/top\/song|v3\/discovery\/recommend\/songs) script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 播放器会员歌曲
# 侧边栏会员等级
http-request ^https:\/\/interface.+music\.163\.com\/eapi\/vipnewcenter\/app\/resource\/newaccountpage script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 侧边栏会员等级
# 首页歌单会员歌曲
http-request ^https?:\/\/interface.+music\.163\.com\/eapi\/(homepage\/|v6\/)?playlist\/ script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 首页歌单会员歌曲
# 会员认证
http-request ^https?:\/\/interface.+music\.163\.com\/eapi\/vipauth\/app\/auth\/(soundquality\/)?query script-path = https://raw.githubusercontent.com/WeiGiegie/666/main/wangyiyun.js, tag = 会员认证

[mitm] 
hostname = *.music.163.com
